	<queryString language="SQL">
		<![CDATA[SELECT
                a.*,
                c.`name` AS `name`,
                d.`name` AS room_name,
                e.`name` AS user_name,
        CASE
                        b.`status` 
                        WHEN 0 THEN
                        b.started_at 
                        WHEN 1 THEN
                        b.checked_in_at 
                        WHEN 2 THEN
                        b.checked_out_at ELSE b.updated_at 
                END AS `start_date`,
        CASE
                        b.`status` 
                        WHEN 0 THEN
                        b.ended_at ELSE NULL 
                END AS `end_date`,
        CASE
                        b.`status` 
                        WHEN 0 THEN
                        'Dipesan' 
                        WHEN 1 THEN
                        'Check In' 
                        WHEN 2 THEN
                        'Check Out' ELSE 'Dibatalkan' 
                END AS `status` 
        FROM
                `consumption_reservation` a
                JOIN reservations b ON a.reservation_id = b.id
                JOIN consumptions c ON a.consumption_id = c.id
                JOIN rooms d ON b.room_id = d.id 
                LEFT JOIN users e ON e.id = b.user_id
        ORDER BY
                start_date DESC]]>
	</queryString>
